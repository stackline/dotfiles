update-notifier=false
# Pin version when saving dependencies
#
#   true:  Add dependencies without caret range(^).
#          ex) "package name": "X.Y.Z"
#
#   false: Add dependencies with caret range (^). This is default.
#          ex) "package name": "^X.Y.Z"
#
save-exact=false
